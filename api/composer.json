{
    "name": "kaponir/console",
    "description": "Скрипт запуска консольных приложений",
    "type": "project",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "require": {
        "php": "^7.0",
        "ext-json": "^7.0",
        "php-di/php-di": "^6.0",
        "symfony/console": "^5.0"
    }
}
